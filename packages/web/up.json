{
  "profile": "wrench-api",
  "regions": ["eu-central-1"],
  "name": "web",
  "lambda": {
    "memory": 512,
    "runtime": "nodejs8.10"
  },
  "stages": {
    "staging": {
      "domain": "beta.wrench.cc",
      "proxy": {
        "command": "NODE_ENV=production node dist/server.js"
      }
    }
  },
  "environment": {
    "API_ENDPOINT": "https://api.wrench.cc/graphql"
  }
}
