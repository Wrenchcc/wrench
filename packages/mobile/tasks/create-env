#!/usr/bin/env bash

set -uex

TARGET_FILE=.env

echo "GRAPHQL_URL=${GRAPHQL_URL}" >> ${TARGET_FILE}
echo "FACEBOOK_CUSTOM_URL_SCHEME=${FACEBOOK_CUSTOM_URL_SCHEME}" >> ${TARGET_FILE}
echo "FACEBOOK_APP_ID=${FACEBOOK_APP_ID}" >> ${TARGET_FILE}
echo "FACEBOOK_APP_SECRET=${FACEBOOK_APP_SECRET}" >> ${TARGET_FILE}
echo "CODE_PUSH_DEPLOYMENT_KEY=${CODE_PUSH_DEPLOYMENT_KEY}" >> ${TARGET_FILE}
echo "SENTRY_DSN=${SENTRY_DSN}" >> ${TARGET_FILE}
echo "ENVIRONMENT=${ENVIRONMENT}" >> ${TARGET_FILE}
