#!/bin/sh
rm -rf ../../.codepush
mkdir ../../.codepush

node ../../node_modules/react-native/local-cli/cli.js ram-bundle --platform ios --dev false --entry-file ./packages/mobile/index.js --bundle-output ../../.codepush/main.jsbundle --assets-dest ../../.codepush 

# appcenter codepush release -d Staging -a Wrench-app/Wrench -c ../../.codepush -t 1.0.0 