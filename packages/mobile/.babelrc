{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "images": "./assets/images",
          "videos": "./assets/videos"
        },
        "extensions": [".js", ".ts"]
      }
    ]
  ],
  "presets": ["module:metro-react-native-babel-preset", "@babel/preset-typescript"]
}
