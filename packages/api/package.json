{
  "name": "@wrench/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- nodemon --exec ts-node src/index.ts",
