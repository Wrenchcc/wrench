{
  "name": "@wrench/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=api* dotenv -e .env -- ts-node-dev --no-notify src/index.ts",
    "compile": "ncc build src/index.ts -e aws-sdk",
    "db:import": "cross-env NODE_ENV=development cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/dbImport.ts",
    "es:create": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/createIndex.ts",
    "es:delete": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/deleteIndex.ts",
    "es:import": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/bulkImport.ts"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^0.9.1",
    "apollo-server-express": "^2.9.3",
    "aws-sdk": "^2.533.0",
    "aws4": "^1.8.0",
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "google-auth-library": "^5.2.2",
    "graphql": "^14.5.7",
    "graphql-depth-limit": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "luxon": "^1.18.1",
    "pg": "^7.12.1",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.19",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.4.3",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/pg": "^7.11.1",
    "@zeit/ncc": "0.20.5",
    "bluebird": "^3.5.5",
    "chokidar": "^3.1.1",
    "cross-env": "^6.0.0",
    "dotenv-cli": "^2.0.1",
    "graphql-log": "^0.1.3",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.6.3"
  }
}
