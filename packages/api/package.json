    "es:create": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/createIndex.ts",
    "es:delete": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/deleteIndex.ts",
    "es:import": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/bulkImport.ts"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "apollo-server-cache-redis": "^1.2.2",
    "apollo-server-express": "^2.18.1",
    "apollo-server-plugin-response-cache": "^0.5.5",
    "aws-sdk": "^2.761.0",
    "axios": "^0.20.0",
    "base-64": "^0.1.0",
    "dataloader": "^1.4.0",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "google-auth-library": "^6.1.0",
    "graphql": "^15.3.0",
    "graphql-depth-limit": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.10.1",
    "lodash.merge": "^4.6.2",
    "luxon": "^1.25.0",
    "ms": "^2.1.2",
    "pg": "^8.3.3",
