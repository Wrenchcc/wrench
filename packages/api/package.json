    "@firebase/app-types": "^0.6.1",
    "@types/express": "^4.17.8",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^14.11.2",
    "@types/pg": "^7.14.5",
    "@vercel/ncc": "^0.24.1",
    "bluebird": "^3.7.2",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^4.0.0",
    "graphql-log": "^0.1.3",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
