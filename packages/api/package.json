{
  "name": "@wrench/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=api* dotenv -e .env -- ts-node-dev --no-notify src/index.ts",
    "build": "ncc build src/index.ts -e aws-sdk -o dist",
    "db:import": "cross-env NODE_ENV=development cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/dbImport.ts",
    "es:create": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/createIndex.ts",
    "es:delete": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/deleteIndex.ts",
    "es:import": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/bulkImport.ts"
  },
  "dependencies": {
    "apollo-server-express": "2.3.3",
    "aws-sdk": "^2.395.0",
    "aws4": "^1.8.0",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "core-js": "3.0.0-beta.3",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "firebase-admin": "^7.0.0",
    "graphql": "^14.1.1",
    "graphql-depth-limit": "^1.1.0",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.4.0",
    "lodash.merge": "^4.6.1",
    "luxon": "^1.10.0",
    "nodemailer": "^5.1.1",
    "pg": "^7.8.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.3.4",
    "typeorm": "next",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.3",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/pg": "^7.4.11",
    "@zeit/ncc": "0.9.0",
    "chokidar": "^2.0.4",
    "cross-env": "^5.2.0",
    "dotenv-cli": "^1.4.0",
    "graphql-log": "^0.1.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.1"
  },
  "workspaces": {
    "nohoist": [
      "typeorm",
      "typeorm/**"
    ]
  }
}
