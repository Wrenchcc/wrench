{
  "name": "@wrench/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- nodemon --exec ts-node src/index.ts",
    "build": "ncc build src/index.ts",
    "db:import": "cross-env NODE_ENV=development cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/dbImport.ts",
    "es:create": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/createIndex.ts",
    "es:delete": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/deleteIndex.ts",
    "es:import": "cross-env DEBUG=task* dotenv -e .env -- ts-node-dev tasks/bulkImport.ts"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "apollo-server-express": "^2.11.0",
    "apollo-server-plugin-response-cache": "^0.4.0",
    "aws-sdk": "^2.632.0",
    "axios": "^0.19.2",
    "base-64": "^0.1.0",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.2",
    "google-auth-library": "^5.10.1",
    "graphql": "^14.6.0",
    "graphql-depth-limit": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.7.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^1.22.0",
    "ms": "^2.1.2",
    "pg": "^7.18.2",
    "ramda": "^0.27.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.24",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.5.2",
    "@types/express": "^4.17.7",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.4",
    "@zeit/ncc": "^0.22.3",
    "bluebird": "^3.7.2",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.2.0",
    "graphql-log": "^0.1.3",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
