{
  "name": "@wrench/ui",
  "version": "1.0.0",
  "private": true,
  "source": "src/index.ts",
  "main": "dist/node/",
  "browser": "dist/web/",
  "react-native": "dist/native/",
  "files": [
    "assets/fonts/native/",
    "dist/"
  ],
  "scripts": {
    "build:storybook": "build-storybook",
    "build:native": "tsc -p tsconfig.native.json",
    "build:web": "tsc -p tsconfig.browser.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build:all": "yarn build:web && yarn build:node && yarn build:native",
    "storybook:web": "start-storybook -p 9001",
    "deploy": "serverless"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "^0.61.5",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/react": "^16.9.22",
    "@types/styled-components": "^5.0.0",
    "@types/styled-system": "^5.1.7",
    "babel-loader": "^8.0.6",
    "storybook-dark-mode": "^0.3.0",
    "typescript": "^3.8.2",
    "serverless": "^1.64.0"
  }
}