{
  "name": "Wrench",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:api": "node api/dist/server.js",
    "build:api": "cross-env NODE_ENV=production cross-env DIR=api webpack",
    "dev:api": "cross-env NODE_ENV=development cross-env DEBUG=build*,api* dotenv -- nodemon -e ts -x ts-node api/index.ts",
    "start:jupiter": "node jupiter/dist/server.js",
    "build:jupiter": "cross-env NODE_ENV=production cross-env DIR=jupiter webpack",
    "dev:jupiter": "cross-env NODE_ENV=development cross-env DEBUG=build*,api* dotenv -e jupiter/.env -- nodemon -e ts -x ts-node jupiter/index.ts"
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "cross-env": "^5.2.0",
    "dotenv-cli": "^1.4.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "ts-loader": "^5.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
