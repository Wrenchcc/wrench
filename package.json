{
  "name": "Wrench",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:packager": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start:ios": "tasks/start_ios",
    "start:android": "tasks/start_android",
    "emulator:start": "tasks/emulator_start",
    "emulator:create": "tasks/emulator_create",
    "emulator:ensureready": "tasks/emulator_ensureready",
    "run:android": "tasks/run_android",
    "run:ios": "react-native run-ios --no-packager --simulator='iPhone X'",
    "assets:transform:svg": "node tasks/transformSVG.js 'assets/images/**/*.svg' 'assets/images' true && node tasks/createIndex.js 'assets/**/*.svg' assets/images",
    "storybook": "storybook start -p 7007",
    "storybook:create": "babel-node tasks/createStoriesEntry.js 'src/**/{,*.}stories{,.ios,.android}.js' 'storybook/stories.js'",
    "translations:create": "babel-node tasks/createTranslations.js 'src/**/{,*.}translations.json' 'src/translations/index.json'",
    "preinstall": "./tasks/facebookSDK"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "human-format": "^0.10.0",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-native": "0.55.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-camera": "^1.1.4",
    "react-native-code-push": "^5.3.4",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.21.5",
    "react-native-fast-image": "^4.0.14",
    "react-native-fbsdk": "^0.7.0",
    "react-native-firebase": "^4.2.0",
    "react-native-i18n": "^2.0.12",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-parsed-text": "^0.0.20",
    "react-native-share": "^1.0.27",
    "react-native-splash-screen": "^3.0.7",
    "react-native-swiper": "^1.5.13",
    "react-native-tab-view": "^1.0.2",
    "react-native-video": "^2.1.1",
    "react-navigation": "^2.2.4",
    "styled-components": "^3.3.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react-native": "^3.4.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "4.0.0",
    "colors": "^1.3.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "faker": "^4.1.0",
    "librsvg-prebuilt": "^0.7.8",
    "prettier": "^1.13.4",
    "prettier-eslint": "^8.8.1",
    "prop-types": "^15.6.1",
    "react-dom": "^16.4.0"
  }
}
