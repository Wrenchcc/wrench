{
  "name": "wrench",
  "version": 2,
  "regions": ["bru1"],
  "builds": [{ "src": "packages/api/src/index.ts", "use": "@now/node-server@0.4.26-canary.7" }],
  "routes": [{ "src": "/api/(.*)", "dest": "packages/api/src/$1" }],
  "env": {
    "APP_AWS_S3_REGION": "us-east-1",
    "APP_AWS_S3_BUCKET": "wrench-files",
    "APP_CDN_DOMAIN": "https://edge-files.wrench.cc",
    "APP_AWS_ACCESS_KEY": "@app_aws_access_key",
    "APP_AWS_SECRET_ACCESS_KEY": "@app_aws_secret_access_key",
    "ACCESS_TOKEN_SECRET": "@access_token_secret",
    "REFRESH_TOKEN_SECRET": "@refresh_token_secret",
    "DB_PASSWORD": "@db_password",
    "DB_USERNAME": "@db_username",
    "DB_HOST": "@db_host",
    "GOOGLE_FIREBASE_API_KEY": "@google_firebase_api_key",
    "DB_DATABASE": "wrench",
    "DB_PORT": "5432",
    "ELASTICSEARCH_DOMAIN": "search-wrench-jgsnkbcupcasu5bxjftpyqrl6q.us-east-1.es.amazonaws.com"
  }
}
